{
  "compilerOptions": {
    "incremental": true,
    "target": "ES6",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "outDir": "./build",
    "paths": {},
  },
  "include": ["src", "src/types"],
  "exclude": ["node_modules", "build"]
}
